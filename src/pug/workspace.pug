                const iOS = !!navigator.platform && /iPad|iPhone|iPod|Mac/.test(navigator.platform);

                const userAgent = navigator.userAgent || navigator.vendor || window.opera;
                const android = !!userAgent && /Android/.test(userAgent);
                const wechat = !!userAgent && /MicroMessenger/.test(userAgent);

                if (iOS) {
                    // Just replace `https://` with `itms://` on your app's AppStore link.
                    document.getElementById('iosApplink').href = "https://apps.apple.com/cn/app/%E6%96%B9%E5%9D%97%E7%AC%94%E8%AE%B0-%E5%8D%B3%E6%97%B6%E7%AC%94%E8%AE%B0-%E7%A8%8D%E5%90%8E%E9%98%85%E8%AF%BB/id1640254686";
                    
