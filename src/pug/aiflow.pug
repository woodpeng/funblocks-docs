        script.
            window.dataLayer = window.dataLayer || [];
            function gtag(){dataLayer.push(arguments);}
            gtag('js', new Date());

            gtag('config', 'G-RYTCZEQK0W');

            function isCNDomain() {
                //- return !window.location.hostname.includes('net');
                return window.location.hostname.includes('cn');
            }

            function getDomain() {
                if(!window.location.hostname.includes('funblocks')) {
                    return 'funblocks.net';
                }
                return window.location.hostname.replace('www.', '');
            }

            function openUrl(url) {
                let newTab = window.open();
                newTab.location.href = url;
            }

            function getAffiliateId() {
                const urlParams = new URLSearchParams(window.location.search);
                return urlParams.get('aid') || '';
            }

            function getRedirectUrl(baseUrl) {
                const affiliateId = getAffiliateId();
                if (!affiliateId) return baseUrl;
                
                // Add affiliate ID to URL if it exists
                const separator = baseUrl.includes('?') ? '&' : '?';
                return `${baseUrl}${separator}aid=${affiliateId}`;
            }

            function toApp() {
                const iOS_phone = !!navigator.platform && /iPhone|iPod/.test(navigator.platform);
                const userAgent = navigator.userAgent || navigator.vendor || window.opera;
                const android = !!userAgent && /Android/.test(userAgent);

                let url = `https://app.${getDomain()}/#/login?source=flow`;
                url = getRedirectUrl(url);

                openUrl(url);
            }

            function toPlans() {
                let url = `https://app.${getDomain()}/#/aiplans?source=flow`;
                url = getRedirectUrl(url);

                openUrl(url);
            }


            function getBrowser() {
                const userAgent = navigator.userAgent;

                if (userAgent.includes("Edg")) {
                    return 'Edge'
                } else if (userAgent.includes("Chrome")) {
                    return 'Chrome'
                }  

                return 'Browser'
            }

            function downloadExtension() {
                const browser = getBrowser();
                if (browser == 'Edge') {
                    window.open("https://microsoftedge.microsoft.com/addons/detail/funblocks-ai-your-ultim/lmmlojdklhcdiefaniakpkhhdmamnigk", '_blank');
                } else if (browser == "Chrome") {
                    window.open("https://chrome.google.com/webstore/detail/coodnehmocjfaandkbeknihiagfccoid", '_blank');
                } else {
                    alert("We only support Chrome and Edge browsers. It is recommended to use Chrome for the best experience.");
                }
            }

            function toggleItem(item) {
                item.classList.toggle('open');
            }
        
